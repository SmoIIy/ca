---
import Layout from '../layouts/Layout.astro';
import Card from '../components/Card.astro';
import '../style.css';
import Footer from '../components/Footer.astro';
import Header from '../components/Header.astro';
import Cover from '../components/Cover.astro';
import Textbox from'../components/Textbox.astro';
---

<Layout title="CA">
	
		<Header/>
			<main>
				<Cover/>
				<Textbox title="Fordele" href="#">
					CA prioriterer dit arbejdsliv og karriere, sikrer tryg udvikling med opdateret viden om erhvervstendenser og kurser, der gør dig attraktiv på arbejdsmarkedet.
				</Textbox>
			</main>
		<Footer/>

</Layout>

<style>
	@media  (min-width: 768px) {
		body {
			overflow-x: hidden;
		}
	}
</style>
